{
    "name": "square1/resized-laravel",
    "description": "Laravel 5 package for resized.co, the on-demand image manipulation service.",
    "keywords": [
        "square1",
        "resized",
        "image manipulation",
        "laravel"
    ],
    "type": "library",
    "homepage": "https://github.com/square1-io/resized-php-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Ciaran Maher",
            "email": "ciaran@square1.io"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "square1/resized": "~1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "laravel/framework": "~5.2",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "phpunit/phpunit-mock-objects": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Square1\\Laravel\\Resized\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Square1\\Laravel\\Resized\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Square1\\Laravel\\Resized\\ResizedServiceProvider"
            ],
            "aliases": {
                "Resized": "Square1\\Laravel\\Resized\\ResizedFacade"
            }
        }
    }
}
